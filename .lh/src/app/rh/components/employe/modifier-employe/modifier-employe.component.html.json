ker-toggle>\n                            <mat-datepicker #picker1></mat-datepicker>\n\n                        </mat-form-field>\n                    </div>\n\n\n                    <div class=\"col-md-3 mb-3\">\n                        <mat-form-field>\n                            <mat-label for=\"dateFinContrat\">Date Fin Contart</mat-label>\n                            <input matInput formControlName=\"dateFinContrat\" id=\"dateFinContrat\"\n                                ng-disabled=\"typeContrat.value=='cdi'\" [matDatepicker]=\"picker\" placeholder=\"DD/MM/YYYY\"\n                                name=\"dateFinContrat\">\n                            <mat-datepicker-toggle matSuffix [for]=\"picker\" value=\"dateFinContrat\">\n                            </mat-datepicker-toggle>\n                            <mat-datepicker #picker></mat-datepicker>\n\n                        </mat-form-field>\n                    </div>\n\n                    <div class=\"col-md-3 mb-3\">\n\n                        <mat-form-field>\n                            <mat-label for=\"soldeConge\"> Solde Congé </mat-label>\n                            <input type=\"number\" matInput formControlName=\"soldeConge\" id=\"soldeConge\" name=\"soldeConge\"\n                                required>\n\n                        </mat-form-field>\n                    </div>\n                </div>\n                <div class=\"row\">\n                    <div class=\"col-md-3 mb-3\">\n                        <mat-form-field>\n                            <mat-label for=\"typeContrat\">Type De Contart</mat-label>\n                            <mat-select formControlName=\"typeContrat\" id=\"typeContrat\" name=\"typeContrat\"\n                                placeholder=\"Type_contart\" value=\"typeContrat\">\n\n                                <mat-option value=\"cdi\">CDI</mat-option>\n                                <mat-option value=\"cdd \">CDD</mat-option>\n                                <mat-option value=\"Etranger\">Etranger</mat-option>\n\n\n                                <mat-option>\n                                </mat-option>\n                            </mat-select>\n                            <!--mat-error *ngIf=\"Informations_Generales_Form.get('Type_contart').invalid\">\n                                {{MessageErreurCategoriePiece()}}</mat-error-->\n                        </mat-form-field>\n\n\n                    </div>\n\n\n\n                </div>\n\n\n                <div>\n                    <button mat-button class=\"btn-primary\" (click)=\"modifiercontrat()\"\n                        style=\" margin-left: 150px;\">Valider</button>\n\n\n                </div>\n\n            </form>\n        </mat-step>\n\n\n\n\n\n\n    </mat-vertical-stepper>\n\n\n</div>"
