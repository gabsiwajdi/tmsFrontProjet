            "content": "<h1 class=\"col-xs-8 text-center\">Modifier Donées Employe </h1>\n\n<div>\n\n    <mat-vertical-stepper [linear]=\"true\" #stepperClient>\n        <mat-step [stepControl]=\"Informations_Contrat_Form\">\n            <form [formGroup]=\"Informations_Contrat_Form\">\n                <ng-template matStepLabel>Informations contrat</ng-template>\n                <div class=\"row\">\n                    <div class=\"col-md-3 mb-3\">\n\n                        <mat-form-field>\n                            <mat-label for=\"Nom_Client\"> Nom du Employé </mat-label>\n                            <input type=\"text\" matInput formControlName=\"nomEmploye\" id=\"nomEmploye\" name=\"nomEmploye\"\n                                required>\n                            <!--mat-error *ngIf=\"Informations_Generales_Form.get('employe').invalid\">\n                                {{MessageErreurNom()}}</mat-error-->\n                        </mat-form-field>\n                    </div>\n                    <div class=\"col-md-3 mb-3\">\n\n                        <mat-form-field>\n                            <mat-label for=\"poste\"> Poste Employe </mat-label>\n                            <input type=\"text\" matInput formControlName=\"posteEmploye\" id=\"posteEmploye\"\n                                name=\"posteEmploye\" required>\n                            <!--mat-error *ngIf=\"Informations_Generales_Form.get('employe').invalid\">\n                                {{MessageErreurNom()}}</mat-error-->\n                        </mat-form-field>\n                    </div>\n\n                    <div class=\"col-md-3 mb-3\">\n\n                        <mat-form-field>\n                            <mat-label for=\"nbreHeurs\"> Nombre Heure </mat-label>\n                            <input type=\"text\" matInput formControlName=\"nbreHeurs\" id=\"nbreHeurs\" name=\"nbreHeurs\"\n                                required>\n                            <!--mat-error *ngIf